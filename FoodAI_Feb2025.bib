@article{ WOS:000710604000001,
Author = {Wang, Dayuan and Zhang, Min and Mujumdar, Arun S. and Yu, Dongxing},
Title = {Advanced Detection Techniques Using Artificial Intelligence in
   Processing of Berries},
Journal = {FOOD ENGINEERING REVIEWS},
Year = {2022},
