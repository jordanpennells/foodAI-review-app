
@article{ WOS:001412220400001,
Author = {Yuan, Xiaoming and Fan, Liying and Jin, Hui and Wu, Qingping and Ding,
   Yu},
Title = {Phage engineering using synthetic biology and artificial intelligence to
   enhance phage applications in food industry},
Journal = {CURRENT OPINION IN FOOD SCIENCE},
Year = {2025},
Volume = {62},
Month = {APR},
Abstract = {The rapid advancements in synthetic biology and artificial intelligence
   (AI) technologies are continuously expanding the application of
   bacteriophages (phages); however, when it comes to the food industry,
   the development of phage applications has been relatively slow due to
   the complex and large-scale nature of the industry. In order to overcome
   these challenges, it is necessary to promote the development of phages
   with new technologies. Here, we introduce the latest synthetic biology
   methods and AI techniques that enhance phage functions and explore their
   potential in promoting phage applications in the food industry.
   Additionally, we discuss the potential of combining synthetic biology
   with AI technologies to overcome the unique challenges when applying
   phages in the food industry in the future.},
