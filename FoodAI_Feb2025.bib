@article{ WOS:000365096500010,
Author = {Ma, Ji and Sun, Da-Wen and Qu, Jia-Huan and Liu, Dan and Pu, Hongbin and
   Gao, Wen-Hong and Zeng, Xin-An},
Title = {Applications of Computer Vision for Assessing Quality of Agri-food
   Products: A Review of Recent Research Advances},
Journal = {CRITICAL REVIEWS IN FOOD SCIENCE AND NUTRITION},
Year = {2016},
Volume = {56},
Number = {1},
Pages = {113-127},
Abstract = {With consumer concerns increasing over food quality and safety, the food
   industry has begun to pay much more attention to the development of
   rapid and reliable food-evaluation systems over the years. As a result,
   there is a great need for manufacturers and retailers to operate
   effective real-time assessments for food quality and safety during food
   production and processing. Computer vision, comprising a nondestructive
   assessment approach, has the aptitude to estimate the characteristics of
   food products with its advantages of fast speed, ease of use, and
   minimal sample preparation. Specifically, computer vision systems are
   feasible for classifying food products into specific grades, detecting
   defects, and estimating properties such as color, shape, size, surface
   defects, and contamination. Therefore, in order to track the latest
   research developments of this technology in the agri-food industry, this
   review aims to present the fundamentals and instrumentation of computer
   vision systems with details of applications in quality assessment of
   agri-food products from 2007 to 2013 and also discuss its future trends
   in combination with spectroscopy.},
Publisher = {TAYLOR \& FRANCIS INC},
Address = {530 WALNUT STREET, STE 850, PHILADELPHIA, PA 19106 USA},
Type = {Review},
