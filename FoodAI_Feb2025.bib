@article{ WOS:000826832300001,
Author = {Tapkire, Mayura D. and Arun, Vanishri},
Title = {Application of artificial intelligence to corelate food formulations to
   disease risk prediction: a comprehensive review},
Journal = {JOURNAL OF FOOD SCIENCE AND TECHNOLOGY-MYSORE},
Year = {2023},
Volume = {60},
Number = {9},
Pages = {2350-2357},
Month = {SEP},
Abstract = {Clinicians and administrators are applying Artificial Intelligence (AI)
   Techniques widely as the promising results of their applications in the
   healthcare have been established. The meaningful impact of the AI
   applications will be limited unless it is coherently applied with human
   diagnosis and inputs from specialist clinician. This will help to
   address limitations and take advantage of the promises of the AI
   techniques. Machine Learning is one of the AI technique that finds high
   relevance in the medicine and health care. This review provides an
   overall glimpse of current practices and research outcomes of the
   application of the AI techniques in the healthcare and medical
   practices. It further describes Machine Learning Techniques in disease
