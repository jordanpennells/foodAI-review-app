@article{ WOS:001085459600001,
Author = {Bhavadharini, B. and Monica, V and Anbarasan, R. and Mahendran, R.},
Title = {Virtual, augmented, and mixed reality as a versatile tool in food
   consumer behavior evaluation: Recent advances in aroma, taste, and
   texture incorporation},
