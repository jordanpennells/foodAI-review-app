@article{ WOS:000382170900007,
Author = {Al-Mahasneh, Majdi and Aljarrah, Mohannad and Rababah, Taha and
   Alu'datt, Muhammad},
Title = {Application of Hybrid Neural Fuzzy System (ANFIS) in Food Processing and
   Technology},
Journal = {FOOD ENGINEERING REVIEWS},
