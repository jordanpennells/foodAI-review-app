@article{ WOS:001062378500001,
Author = {Shi, Ce and Zhao, Zhiyao and Jia, Zhixin and Hou, Mengyuan and Yang,
   Xinting and Ying, Xiaoguo and Ji, Zengtao},
Title = {Artificial neural network-based shelf life prediction approach in the
   food storage process: A review},
