@article{ WOS:000788127400007,
Author = {Wang, Wei and Min, Weiqing and Li, Tianhao and Dong, Xiaoxiao and Li,
   Haisheng and Jiang, Shuqiang},
Title = {A review on vision-based analysis for automatic dietary assessment},
Journal = {TRENDS IN FOOD SCIENCE \& TECHNOLOGY},
Year = {2022},
