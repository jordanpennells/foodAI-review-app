@article{ WOS:000765733200001,
Author = {Malekjani, Narjes and Jafari, Seid Mahdi},
Title = {Intelligent and Probabilistic Models for Evaluating the Release of Food
   Bioactive Ingredients from Carriers/Nanocarriers},
Journal = {FOOD AND BIOPROCESS TECHNOLOGY},
