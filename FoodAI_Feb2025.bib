@article{ WOS:001321639100001,
Author = {Queiroz, L. P. and Nogueira, I. B. R. and Ribeiro, A. M.},
Title = {Flavor Engineering: A comprehensive review of biological foundations, AI
   integration, industrial development, and socio-cultural dynamics},
Journal = {FOOD RESEARCH INTERNATIONAL},
