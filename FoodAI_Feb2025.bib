@article{ WOS:000784063400001,
Author = {An, Dong and Zhang, Liu and Liu, Zhe and Liu, Jincun and Wei, Yaoguang},
Title = {Advances in infrared spectroscopy and hyperspectral imaging combined
   with artificial intelligence for the detection of cereals quality},
Journal = {CRITICAL REVIEWS IN FOOD SCIENCE AND NUTRITION},
