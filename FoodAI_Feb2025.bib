@article{ WOS:001196244000001,
Author = {Anwar, H. and Anwar, T.},
Title = {APPLICATION OF ELECTRONIC NOSE AND MACHINE LEARNING IN DETERMINING
   FRUITS QUALITY: A REVIEW},
Journal = {JOURNAL OF ANIMAL AND PLANT SCIENCES-JAPS},
Year = {2024},
